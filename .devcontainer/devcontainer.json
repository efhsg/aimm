{
  "name": "AIMM (Yii)",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "aimm_yii",
  "workspaceFolder": "/var/www/html",
  "workspaceFile": "/var/www/html/aimm.code-workspace",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client"
      ],
      "settings": {
        "intelephense.environment.includePaths": [
          "/var/www/html/yii/vendor"
        ],
        "intelephense.environment.phpVersion": "8.5"
      }
    }
  }
}
